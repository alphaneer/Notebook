	langid = {english},
	keywords = {Functional genomics, Genomic analysis, Nuclear organization},
	file = {Full Text PDF:C\:\\Users\\DELL\\Zotero\\storage\\FEX5SPJF\\Li 等. - 2017 - GRID-seq reveals the global RNA-chromatin interact.pdf:application/pdf;Snapshot:C\:\\Users\\DELL\\Zotero\\storage\\S8C43U6Q\\nbt.3968.html:text/html}
}

@article{zappia_splatter:_2017,

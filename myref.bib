@article{clevenger_single_2015,
	title = {Single Nucleotide Polymorphism Identification in Polyploids: A Review, Example, and Recommendations},
	volume = {8},
	issn = {1674-2052},
	url = {http://www.sciencedirect.com/science/article/pii/S1674205215001306},
	doi = {10.1016/j.molp.2015.02.002},
	shorttitle = {Single Nucleotide Polymorphism Identification in Polyploids},
	abstract = {Understanding the relationship between genotype and phenotype is a major biological question and being able to predict phenotypes based on molecular genotypes is integral to molecular breeding. Whole-genome duplications have shaped the history of all flowering plants and present challenges to elucidating the relationship between genotype and phenotype, especially in neopolyploid species. Although single nucleotide polymorphisms ({SNPs}) have become popular tools for genetic mapping, discovery and application of {SNPs} in polyploids has been difficult. Here, we summarize common experimental approaches to {SNP} calling, highlighting recent polyploid successes. To examine the impact of software choice on these analyses, we called {SNPs} among five peanut genotypes using different alignment programs ({BWA}-mem and Bowtie 2) and variant callers ({SAMtools}, {GATK}, and Freebayes). Alignments produced by Bowtie 2 and {BWA}-mem and analyzed in {SAMtools} shared 24.5\% concordant {SNPs}, and {SAMtools}, {GATK}, and Freebayes shared 1.4\% concordant {SNPs}. A subsequent analysis of simulated Brassica napus chromosome 1A and 1C genotypes demonstrated that, of the three software programs, {SAMtools} performed with the highest sensitivity and specificity on Bowtie 2 alignments. These results, however, are likely to vary among species, and we therefore propose a series of best practices for {SNP} calling in polyploids.},
	pages = {831--846},
	number = {6},
	journaltitle = {Molecular Plant},
	shortjournal = {Molecular Plant},
	author = {Clevenger, Josh and Chavarro, Carolina and Pearl, Stephanie A. and Ozias-Akins, Peggy and Jackson, Scott A.},
	urldate = {2017-12-05},
	date = {2015-06-01},
	keywords = {genomics, homeolog, next-generation sequencing ({NGS}), peanut, polyploid, single nucleotide polymorphism ({SNP})},
	file = {Clevenger et al_2015_Single Nucleotide Polymorphism Identification in Polyploids.pdf:C\:\\Users\\DELL\\Zotero\\storage\\8QSFSZK4\\Clevenger et al_2015_Single Nucleotide Polymorphism Identification in Polyploids.pdf:application/pdf}
}

@article{macnair_zinc_1999,
	title = {Zinc tolerance and hyperaccumulation are genetically independent characters.},
	volume = {266},

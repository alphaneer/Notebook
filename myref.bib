  keywords = {adaptation,environmental association,genome resequencing,Pool-Seq,pooled sequencing,population genomics},
  file = {C:\\Users\\DELL\\Zotero\\storage\\VZ7QI6J4\\Fischer et al_2013_Population genomic footprints of selection and associations with climate in.pdf}
}


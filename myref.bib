@article{khatri_ten_2012,
	title = {Ten Years of Pathway Analysis: Current Approaches and Outstanding Challenges},
	volume = {8},
	issn = {1553-7358},
	url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1002375},

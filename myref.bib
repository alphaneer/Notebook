  shortjournal = {BMC Bioinformatics},
  author = {Melo, Arthur T. O. and Bartaula, Radhika and Hale, Iago},
  date = {2016-01-12},
  file = {C:\\Users\\DELL\\Zotero\\storage\\88UQM2AU\\Melo et al_2016_GBS-SNP-CROP.pdf},
  eprinttype = {pmid},
  eprint = {26754002},
  pmcid = {PMC4709900}
}


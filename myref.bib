	urldate = {2017-09-19},
	date = {2016-09},
	langid = {english},
	keywords = {Plant genetics, Population genetics},
	file = {Novikova et al_2016_Sequencing of the genus Arabidopsis identifies a complex history of.pdf:C\:\\Users\\DELL\\Zotero\\storage\\MUMVWQNQ\\Novikova et al_2016_Sequencing of the genus Arabidopsis identifies a complex history of.pdf:application/pdf;Novikova et al_2016_Sequencing of the genus Arabidopsis identifies a complex history of.pdf:C\:\\Users\\DELL\\Zotero\\storage\\UFKN5UKW\\Novikova et al_2016_Sequencing of the genus Arabidopsis identifies a complex history of.pdf:application/pdf;Snapshot:C\:\\Users\\DELL\\Zotero\\storage\\9NIGTER4\\ng.3617.html:text/html}

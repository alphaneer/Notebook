	volumes = {Supplement C},
	pages = {79--98},
	booktitle = {Methods in Enzymology},
	publisher = {Academic Press},
	author = {Borodina, Tatiana and Adjaye, James and Sultan, Marc},
	editor = {Jameson, Daniel and Verma, Malkhey and Westerhoff, Hans V.},
	date = {2011-01-01},
	note = {{DOI}: 10.1016/B978-0-12-385118-5.00005-0},
	keywords = {{RNA}-seq, next generation sequencing, {RNA} sequencing, {cDNA} synthesis, Fragmentation, {polyARNA}},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\DELL\\Zotero\\storage\\BEW6VCX9\\Borodina 等. - 2011 - Chapter five - A Strand-Specific Library Preparati.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\DELL\\Zotero\\storage\\JNC7ZMMF\\B9780123851185000050.html:text/html}
}


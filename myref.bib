	keywords = {single cell, alternative splicing, {RNA} processing, stem cells, neuron, differentiation, post-transcription, modality, bimodality},
	file = {Snapshot:C\:\\Users\\DELL\\Zotero\\storage\\BIU5FU8X\\S1097-2765(17)30405-7.html:text/html}

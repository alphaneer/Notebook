@article{McCormickRIGRecalibrationInterrelation2015,
  langid = {english},
  title = {{{RIG}}: {{Recalibration}} and {{Interrelation}} of {{Genomic Sequence Data}} with the {{GATK}}},
  volume = {5},
  rights = {Copyright © 2015 McCormick et al.. This is an open-access article distributed under the terms of the Creative Commons Attribution Unported License (http://creativecommons.org/licenses/by/3.0/), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.},
  issn = {2160-1836},
  url = {http://www.g3journal.org/content/5/4/655},
  doi = {10.1534/g3.115.017012},
  shorttitle = {{{RIG}}},
  abstract = {Recent advances in variant calling made available in the Genome Analysis Toolkit (GATK) enable the use of validated single-nucleotide polymorphisms and indels to improve variant calling. However, large collections of variants for this purpose often are unavailable to research communities. We introduce a workflow to generate reliable collections of single-nucleotide polymorphisms and indels by leveraging available genomic resources to inform variant calling using the GATK. The workflow is demonstrated for the crop plant Sorghum bicolor by (i) generating an initial set of variants using reduced representation sequence data from an experimental cross and association panels, (ii) using the initial variants to inform variant calling from whole-genome sequence data of resequenced individuals, and (iii) using variants identified from whole-genome sequence data for recalibration of the reduced representation sequence data. The reliability of variants called with the workflow is verified by comparison with genetically mappable variants from an independent sorghum experimental cross. Comparison with a recent sorghum resequencing study shows that the workflow identifies an additional 1.62 million high-confidence variants from the same sequence data. Finally, the workflow’s performance is validated using Arabidopsis sequence data, yielding variant call sets with 95\% sensitivity and 99\% positive predictive value. The Recalibration and Interrelation of genomic sequence data with the GATK (RIG) workflow enables the GATK to accurately identify genetic variation in organisms lacking validated variant resources.},
  number = {4},
  journaltitle = {G3: Genes, Genomes, Genetics},
  author = {McCormick, Ryan F. and Truong, Sandra K. and Mullet, John E.},
  urldate = {2017-12-05},
  date = {2015-04-01},
  pages = {655--665},
  keywords = {GATK,genomic sequence data,variant calling},
  file = {C:\\Users\\DELL\\Zotero\\storage\\FXDUN2X3\\McCormick et al_2015_RIG.pdf;C:\\Users\\DELL\\Zotero\\storage\\LB78GXEE\\655.html},
  eprinttype = {pmid},
  eprint = {25681258}
}

@article{ClevengerSingleNucleotidePolymorphism2015,
  title = {Single {{Nucleotide Polymorphism Identification}} in {{Polyploids}}: {{A Review}}, {{Example}}, and {{Recommendations}}},
  volume = {8},
  issn = {1674-2052},
  url = {http://www.sciencedirect.com/science/article/pii/S1674205215001306},
  doi = {10.1016/j.molp.2015.02.002},
  shorttitle = {Single {{Nucleotide Polymorphism Identification}} in {{Polyploids}}},
  abstract = {Understanding the relationship between genotype and phenotype is a major biological question~and being able to predict phenotypes based on molecular genotypes is integral to molecular breeding. Whole-genome duplications have shaped the history of all flowering plants and present~challenges to elucidating the relationship between genotype and phenotype, especially in neopolyploid species. Although single nucleotide polymorphisms (SNPs) have become popular tools for genetic mapping, discovery and application of SNPs in polyploids has been difficult. Here, we summarize common experimental approaches to SNP calling, highlighting recent polyploid successes. To examine the impact of software choice on these analyses, we called SNPs among five peanut genotypes using different alignment programs (BWA-mem and Bowtie 2) and variant callers (SAMtools, GATK, and Freebayes). Alignments produced by Bowtie 2 and BWA-mem and analyzed in SAMtools shared 24.5\% concordant SNPs, and SAMtools, GATK, and Freebayes shared 1.4\% concordant SNPs. A subsequent analysis of simulated Brassica napus chromosome 1A and 1C genotypes demonstrated that, of the three software programs, SAMtools performed with the highest sensitivity and specificity on Bowtie 2 alignments. These results, however, are likely to vary among species, and we therefore propose a series of best practices for SNP calling in polyploids.},
  number = {6},
  journaltitle = {Molecular Plant},
  shortjournal = {Molecular Plant},
  author = {Clevenger, Josh and Chavarro, Carolina and Pearl, Stephanie~A. and Ozias-Akins, Peggy and Jackson, Scott~A.},
  urldate = {2017-12-05},
  date = {2015-06-01},
  pages = {831--846},
  keywords = {genomics,homeolog,next-generation sequencing (NGS),peanut,polyploid,single nucleotide polymorphism (SNP)},
  file = {C:\\Users\\DELL\\Zotero\\storage\\8QSFSZK4\\Clevenger et al_2015_Single Nucleotide Polymorphism Identification in Polyploids.pdf}
}

@article{MacnairZinctolerancehyperaccumulation1999,
  title = {Zinc Tolerance and Hyperaccumulation Are Genetically Independent Characters.},

  pages = {1077--1082},
  keywords = {Plant genetics,Population genetics},
  file = {C:\\Users\\DELL\\Zotero\\storage\\MUMVWQNQ\\Novikova et al_2016_Sequencing of the genus Arabidopsis identifies a complex history of.pdf;C:\\Users\\DELL\\Zotero\\storage\\9NIGTER4\\ng.3617.html}

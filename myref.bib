  file = {C:\\Users\\DELL\\Zotero\\storage\\RMK4W4YR\\Li 等。 - 2009 - SNP detection for massively parallel whole-genome .pdf;C:\\Users\\DELL\\Zotero\\storage\\5F3TW3LI\\1124.html},
  eprinttype = {pmid},

@article{HuangGenomewideassociationstudies2010,
  title = {Genome-Wide Association Studies of 14 Agronomic Traits in Rice Landraces},
  volume = {42},
  issn = {1061-4036},
  doi = {10.1038/ng.695},
  abstract = {Uncovering the genetic basis of agronomic traits in crop landraces that have adapted to various agro-climatic conditions is important to world food security. Here we have identified similar to 3.6 million SNPs by sequencing 517 rice landraces and constructed a high-density haplotype map of the rice genome using a novel data-imputation method. We performed genome-wide association studies (GWAS) for 14 agronomic traits in the population of Oryza sativa indica subspecies. The loci identified through GWAS explained similar to 36\% of the phenotypic variance, on average. The peak signals at six loci were tied closely to previously identified genes. This study provides a fundamental resource for rice genetics research and breeding, and demonstrates that an approach integrating second-generation genome sequencing and GWAS can be used as a powerful complementary strategy to classical biparental cross-mapping for dissecting complex traits in rice.},
  number = {11},
  journaltitle = {Nature Genetics},
  author = {Huang, Xuehui and Wei, Xinghua and Sang, Tao and Zhao, Qiang and Feng, Qi and Zhao, Yan and Li, Canyang and Zhu, Chuanrang and Lu, Tingting and Zhang, Zhiwu and Li, Meng and Fan, Danlin and Guo, Yunli and Wang, Ahong and Wang, Lu and Deng, Liuwei and Li, Wenjun and Lu, Yiqi and Weng, Qijun and Liu, Kunyan and Huang, Tao and Zhou, Taoying and Jing, Yufeng and Li, Wei and Lin, Zhang and Buckler, Edward S. and Qian, Qian and Zhang, Qi-Fa and Li, Jiayang and Han, Bin},

  date = {2013-04-06},
  pages = {9385--9390},
  file = {C:\\Users\\DELL\\Zotero\\storage\\8V3WBIWC\\Rosenblum 等。 - 2013 - Complex history of the amphibian-killing chytrid f.pdf;C:\\Users\\DELL\\Zotero\\storage\\CGQNZL7X\\9385.html},

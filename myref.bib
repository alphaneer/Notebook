@article{parkhomchuk_transcriptome_2009,
	title = {Transcriptome analysis by strand-specific sequencing of complementary {DNA}},
	volume = {37},
	issn = {0305-1048},
	url = {https://academic.oup.com/nar/article/37/18/e123/1085684/Transcriptome-analysis-by-strand-specific},

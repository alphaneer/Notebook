@article{gao_genetic_2017,
	title = {Genetic diversity and structure of wild and cultivated Amorphophallus paeoniifolius populations in southwestern China as revealed by {RAD}-seq},
	volume = {7},
	rights = {2017 The Author(s)},
	issn = {2045-2322},

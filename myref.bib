	pages = {166--176},
	number = {2},
	journaltitle = {Developmental Cell},
	shortjournal = {Developmental Cell},
	author = {Bardou, Florian and Ariel, Federico and Simpson, Craig G. and Romero-Barrios, Natali and Laporte, Philippe and Balzergue, Sandrine and Brown, John W. S. and Crespi, Martin},

	number = {1},
	journaltitle = {Molecular Cell},
	shortjournal = {Molecular Cell},
	author = {Song, Yan and Botvinnik, Olga B. and Lovci, Michael T. and Kakaradov, Boyko and Liu, Patrick and Xu, Jia L. and Yeo, Gene W.},
	urldate = {2017-07-09},
